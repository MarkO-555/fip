cd /etc
mkdir default
bget group
bget hostname
bget inittab
bget passwd
bget first
bget rc
chmod 0755 rc
bget rc.chk
chmod 0755 rc.chk
bget rc.first
chmod 0755 rc.first
mkdir skel
cd skel
bget skel/.profile
chmod 0755 .profile
cd /bin
bget shutdown
chmod 0755 shutdown
chmod 04755 /bin/passwd
mkdir /home
exit
